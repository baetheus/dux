{
  "name": "@nll/dux",
  "version": "5.1.1-build-0.0",
  "description": "Flux and flux-like tools",
  "main": "dist/es5/index.js",
  "module": "dist/es6/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run build-es6 && npm run build-es5",
    "build-es6": "tsc",
    "build-es5": "tsc --build tsconfig.es5.json",
    "watch": "tsc --watch",
    "test": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "release": "npm run docs && npx standard-version",
    "docs": "npx docs-ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nullpub/dux.git"
  },
  "keywords": [
    "redux",
    "ngrx",
    "flux"
  ],
  "author": "Brandon Blaylock (brandon@null.pub)",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/nullpub/dux/issues"
  },
  "homepage": "https://github.com/nullpub/dux#readme",
  "dependencies": {
    "fp-ts": "^2",
    "monocle-ts": "^2",
    "rxjs": "^6"
  },
  "devDependencies": {
    "docs-ts": "^0.2.0",
    "standard-version": "^5.0.0",
    "typescript": "^3.3.3333"
  }
}
